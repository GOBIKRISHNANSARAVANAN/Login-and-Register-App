<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity4">
    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".MainActivity4"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Name"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:enabled="false"/>

        <EditText
            android:id="@+id/text2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:enabled="false"/>

        <TextView
            android:id="@+id/textView15"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="College Name"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:enabled="false"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="phone"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="phone"
            android:enabled="false"/>

        <TextView
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Event Name"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:enabled="false"/>

        <TextView
            android:id="@+id/textView18"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="PPT Topic"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text6"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:enabled="false"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Food Token No"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text7"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:enabled="false"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Food Token 1"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text8"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:text=""
            android:inputType="textPersonName"
            android:enabled="false"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Food Token 2"
            android:textSize="36sp" />

        <EditText
            android:id="@+id/text9"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:text=""
            android:inputType="textPersonName"
            android:enabled="false"/>
        <Button
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/button"
            android:text="Generate Food Token"
            android:ems="10"/>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/img"
            android:ems="10"
            android:layout_marginBottom="25dp"/>
        <Button
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/button1"
            android:text="Attempt Code Debuging"
            android:ems="10"
            tools:ignore="DuplicateIds" />
        <Button
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/button2"
            android:text="Attempt Quiz"
            android:ems="10"
            tools:ignore="DuplicateIds" />
        <Button
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/button3"
            android:text="Online Presentation"
            android:ems="10"
            tools:ignore="DuplicateIds" />
        <EditText
            android:id="@+id/text10"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:text=""
            android:inputType="textPersonName"
            android:enabled="false"/>

    </LinearLayout>


</ScrollView>